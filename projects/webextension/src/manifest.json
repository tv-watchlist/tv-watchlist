{
    "name": "TV WatchList",
    "description": "Track favorite TV shows and episodes by creating a personal WatchList.",
    "version": "3.1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "TV WatchList v3.0.0",
        "default_popup": "/index.html#/popup",
        "default_icon": {
            "16": "myTvQ1x16.png",
             "32": "myTvQ1x32.png",
             "48": "myTvQ1x48.png",
             "128": "myTvQ1x128.png"
          }
    },
    "background": {
        "page": "/index.html#/background",
        "persistent": false
    },
    "options_page": "/index.html#/options",
    "icons": {
        "16": "myTvQ1x16.png",
        "32": "myTvQ1x32.png",
        "48": "myTvQ1x48.png",
        "128": "myTvQ1x128.png"
      },
    "offline_enabled": true
}
